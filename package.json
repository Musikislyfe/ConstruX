{
  "name": "fope-construction-management",
  "version": "1.0.0",
  "description": "Field Optimization Protocol Engine - Construction management system with GPS tracking, photo documentation, and AI assistance",
  "main": "src/backend/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:frontend": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "db:setup": "node scripts/setup-database.js",
    "db:seed": "node scripts/seed-database.js",
    "start": "node src/backend/server.js"
  },
  "keywords": [
    "construction",
    "field-management",
    "gps-tracking",
    "photo-documentation",
    "ai-assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "multer": "^1.4.5-lts.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "@google/generative-ai": "^0.2.0",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "vite": "^5.0.11",
    "nodemon": "^3.0.3",
    "concurrently": "^8.2.2"
  }
